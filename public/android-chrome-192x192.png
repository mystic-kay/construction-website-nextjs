      <rect x="200" y="160" width="20" height="35"/>
      <rect x="240" y="160" width="20" height="35"/>
      <rect x="280" y="160" width="20" height="35"/>

      <!-- Row 2 -->
      <rect x="200" y="210" width="20" height="35"/>
