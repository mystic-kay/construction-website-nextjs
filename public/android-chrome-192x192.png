      <rect x="280" y="210" width="20" height="35"/>

      <!-- Row 3 -->
      <rect x="200" y="260" width="20" height="35"/>
      <rect x="240" y="260" width="20" height="35"/>
      <rect x="280" y="260" width="20" height="35"/>

      <!-- Row 4 -->
      <rect x="200" y="310" width="20" height="35"/>
      <rect x="240" y="310" width="20" height="35"/>
      <rect x="280" y="310" width="20" height="35"/>

      <!-- Row 5 -->
      <rect x="200" y="360" width="20" height="35"/>
      <rect x="240" y="360" width="20" height="35"/>
      <rect x="280" y="360" width="20" height="35"/>

      <!-- Door -->
      <rect x="230" y="380" width="52" height="60"/>
    </g>
  </g>
</svg>
